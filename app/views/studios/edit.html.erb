<h1>Edit Studio Info</h1>
<%= form_for @studio do |f| %>
<%= f.text_field :name, placeholder: "Studio Name" %>
<%= f.text_field :address, placeholder: "Physical Address" %>
<%= f.text_field :phone, placeholder: "Phone Number" %>
<%= f.text_field :website, placeholder: "Website Address" %>
<%= f.text_field :image_url, placeholder: "Image URL" %>
<%= f.submit "Submit Studio" %>
<% end %>